{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "module": "ES2022",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES2022",
    "verbatimModuleSyntax": true
  }
}